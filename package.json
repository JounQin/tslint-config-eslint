{
  "name": "tslint-config-eslint",
  "version": "0.3.0",
  "description": "Yet another TSLint Configuration which disables all rules which has been handled by `eslint` or `@typescript-eslint`.",
  "repository": "git@github.com:rx-ts/tslint-config-eslint.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "main": "base.json",
  "files": [
    "base.json",
    "prettier.json",
    "sonar.json"
  ],
  "scripts": {
    "build": "ts-node build",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,ts -f friendly",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --noEmit",
    "release": "standard-version -a --release-as",
    "typecov": "type-coverage"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=2.30.0",
    "eslint-plugin-sonar": "^0.2.0",
    "tslint": ">=5.0.0",
    "typescript": ">=3.0.0"
  },
  "devDependencies": {
    "@1stg/common-config": "^1.2.9",
    "@1stg/tslint-config": "^1.0.1",
    "@types/node": "^14.14.31",
    "core-js": "^3.9.1",
    "npm-run-all": "^4.1.5",
    "standard-version": "^9.1.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "type-coverage": "^2.16.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "resolutions": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  },
  "typeCoverage": {
    "atLeast": 100,
    "cache": true,
    "detail": true,
    "ignoreFiles": [
      "*.d.ts"
    ],
    "strict": true,
    "update": true
  }
}
